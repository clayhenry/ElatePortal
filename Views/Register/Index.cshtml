
@model ElatePortal.Models.Profile

@{
    ViewData["Title"] = "Register";


}

<h2>Register</h2>


<form asp-controller="Home" asp-action="Register" method="post">
    <div class="row">
        <div class="col-md-4">
            <strong>Name:</strong>  @ViewData["name"] <strong>Email:</strong> @ViewData["email"]



            <div class="form-group">
                <label asp-for="About" class="control-label"></label>
                <textarea asp-for="About" class="form-control" >@Model.About  </textarea>
            </div>
            <div class="form-group">
                <input type="submit" value="Register" class="btn btn-default" />
            </div>
            </div>
        </div>

</form> 
